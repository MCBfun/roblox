local cycleTeleportButton = createFuncBtn(mainFrame, UDim2.new(0,5,0,75), "è½®æµä¼ é€")
local autoPipeButton = createFuncBtn(mainFrame, UDim2.new(0,5,0,120), "è‡ªåŠ¨ç®¡é“")

